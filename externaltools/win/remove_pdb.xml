<?xml version="1.0" encoding="UTF-8"?>
<externaltool id="remove_pdb">
    <description>Remove bad PDB file</description>
    <displayname>Remove PDB</displayname>
    <category>Playrix</category>
    <executable output="ignore" error="showinpane" modifiesdocument="no">
        <path>bash</path>
        <arguments>-c &quot;rm bin/%{ActiveProject:BuildConfig:Name}/%{ActiveProject:Name}.pdb&quot;</arguments>
        <workingdirectory>%{ActiveProject:BuildConfig:Path}</workingdirectory>
    </executable>
</externaltool>
