<?xml version="1.0" encoding="UTF-8"?>
<externaltool id="windows_terminal">
    <description>Open Windows Terminal in the current project directory</description>
    <displayname>Windows Terminal</displayname>
    <category></category>
    <executable output="ignore" error="showinpane" modifiesdocument="no">
        <path>wt</path>
        <arguments>-d &quot;%{ActiveProject:Path}&quot;</arguments>
    </executable>
</externaltool>
