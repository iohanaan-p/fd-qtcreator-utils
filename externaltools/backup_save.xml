<externaltool id="backup_save">
    <description>Backup save file</description>
    <displayname>Backup Save</displayname>
    <category>Playrix</category>
    <executable output="ignore" error="showinpane" modifiesdocument="no">
        <path>bash</path>
        <arguments>-c &quot;mkdir -p $HOME/Documents/Playrix/backup &amp;&amp; cp $HOME/Documents/Playrix/{%{ActiveProject:Name},backup}/GameInfo.xml&quot;</arguments>
        <workingdirectory>%{ActiveProject:Path}</workingdirectory>
    </executable>
</externaltool>
