<externaltool id="restore_save">
    <description>Restore save file from backup</description>
    <displayname>Restore Save</displayname>
    <category>Playrix</category>
    <executable output="ignore" error="showinpane" modifiesdocument="no">
        <path>bash</path>
        <arguments>-c &quot;cp $HOME/Documents/Playrix/{backup,%{ActiveProject:Name}}/GameInfo.xml&quot;</arguments>
        <workingdirectory>%{ActiveProject:Path}</workingdirectory>
    </executable>
</externaltool>
